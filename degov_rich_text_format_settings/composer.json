{
  "name": "degov_rich_text_format_settings",
  "type": "drupal-module",
  "description": "Adds default configuration for rich text format.",
  "license": "GPL-2.0+",
  "homepage": "https://www.degov.org",
  "require": {
    "drupal/linkit": "^4.3"
  },
  "extra": {
    "installer-paths": {
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ]
    },
    "patches": {
      "drupal/entity_embed": {
        "2511404 - Image entities/fields embedded using Entity Embed cannot be linked in CKEditor":
        "https://www.drupal.org/files/issues/entity_embed_links-2511404-31.patch"
      }
    }
  }
}
